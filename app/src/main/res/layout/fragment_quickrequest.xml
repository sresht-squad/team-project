<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:id="@+id/tvServerHelp"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/alef"
        android:gravity="center"
        android:text="Server Assistance"
        android:textSize="19sp"
        app:layout_constraintEnd_toEndOf="@+id/ivServerHelp"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/ivServerHelp"
        app:layout_constraintTop_toBottomOf="@+id/ivServerHelp" />

    <ImageView
        android:id="@+id/ivServerHelp"
        android:layout_width="153dp"
        android:layout_height="159dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="100dp"
        app:layout_constraintEnd_toStartOf="@+id/ivWater"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/waiter_icon" />

    <TextView
        android:id="@+id/tvWater"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/alef"
        android:gravity="center"
        android:text="Water"
        android:textSize="19sp"
        app:layout_constraintEnd_toEndOf="@+id/ivWater"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/ivWater"
        app:layout_constraintTop_toBottomOf="@+id/ivWater" />

    <ImageView
        android:id="@+id/ivWater"
        android:layout_width="148dp"
        android:layout_height="157dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="104dp"
        android:layout_marginEnd="7dp"
        android:layout_marginRight="7dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/ivServerHelp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/drink_icon" />

    <TextView
        android:id="@+id/tvCheck"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:layout_marginBottom="8dp"
        android:fontFamily="@font/alef"
        android:gravity="center"
        android:text="Check"
        android:textSize="19sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/ivCheck"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/ivCheck"
        app:layout_constraintTop_toBottomOf="@+id/ivCheck"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/ivCheck"
        android:layout_width="144dp"
        android:layout_height="178dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="88dp"
        app:layout_constraintEnd_toStartOf="@+id/ivToGoBox"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvServerHelp"
        app:srcCompat="@drawable/check_icon_blue" />

    <TextView
        android:id="@+id/tvToGoBox"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:layout_marginBottom="8dp"
        android:fontFamily="@font/alef"
        android:gravity="center"
        android:text="To Go Boxes"
        android:textSize="19sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/ivToGoBox"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/ivToGoBox"
        app:layout_constraintTop_toBottomOf="@+id/ivToGoBox"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/ivToGoBox"
        android:layout_width="137dp"
        android:layout_height="176dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="88dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/ivCheck"
        app:layout_constraintTop_toBottomOf="@+id/tvWater"
        app:srcCompat="@drawable/toGoBox_icon" />



</androidx.constraintlayout.widget.ConstraintLayout>